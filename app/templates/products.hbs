<div class="panel panel-default">
  <table class="table">
     <thead>
      <tr>
         <th class="col-md-4">Name</th>
         <th class="col-md-4">Description</th>
         <th class="col-md-4">Price</th>
      </tr>
   </thead>

   <tbody>
   {{#each model as |product|}}
      <tr>
         <td class="col-md-4">{{product.name}}</td>
         <td class="col-md-4">{{product.description}}</td>
         <td class="col-md-4">{{product.price}}</td>
      </tr>
   {{/each}}
   </tbody>

  </table>
</div>
{{infinity-loader infinityModel=model loadingText="Loading..." loadedText="Complete List of Products"}}